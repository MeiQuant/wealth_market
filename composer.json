{
    "config" : {
        "secure-http" : false
    },

    "require": {
        "illuminate/database": "5.0.*",
        "illuminate/events": "5.0.*",
        "symfony/debug": "2.6.*",
        "symfony/var-dumper": "2.6.*",
        "guzzlehttp/guzzle": "6.*",
        "overtrue/wechat": "~3.3"
    },

    "autoload" : {
        "classmap": [
            "application/library",
            "application/controllers",
            "application/plugins",
            "application/views"
        ]
    },

    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
